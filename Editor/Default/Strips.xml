<strips>
	<!--
	Attributes used in default XML file:
	i="default image". Eg image resource name.
	t="default text". If this attribute is missing, creates text from name, for example "File_NewScript" -> "New Script".
	tt="tooltip". If this attribute is missing, for tooltip uses t.
	color="ColorName" - text color.
	color="0xRRGGBB" - text color.
	hk="hotkey" - hotkey (keyboard shortcut). Activates the hotkey and displays it. Example: Ctrl+H. See documentation of function Catkeys.Input.Misc.ReadHotkeyString.
	hkText="other hotkeys" - text to append to the hotkey display text. If hk not used - text to display as hotkey. Use this for hotkeys that your application implements not through the MenuStrip.
	style="ToolStripItemDisplayStyle" - 1 text, 2 image, 3 image+text.
	hide="" - hidden (always is on the overflow strip).
	dd="SubmenuName" - name of another submenu that is used for this item too. That submenu must be above in XML.
	dd="" - creates submenu with single item - separator. The caller can find it in the Submenus dictionary and fill it at any time later.
	type="edit" - creates text edit box.
	type="combo" - creates combo box.
	cue="edit or combo box cue text".
	-->
	<!--
	Attributes used in customizations file, which is auto-created:
	i2="custom icon path". Overrides i.
	t2="custom text". Overrides t.
	color, hk, style, hide.
	-->
	<!--
	Tags:
	<sep/> - separator.
	Toolbar/menu/submenu tags must be unique.
	Other tags don't have to be unique. For example, use the same tag if you use the same Click event handler for a menu item and toolbar button. However duplicate menu items should not be used; then their customization behavior is undefined.
	-->
	<!--
	Menu bar and its drop-down menus and submenus.
	Must be before toolbars.
	Users cannot add/delete/reorder items.
	Users can copy items to custom toolbars.
	Users can change some item properties.
	-->
	<Menu>
		<Menu_File>
			<File_New>
				<File_NewScript i="_new" hk="Ctrl+N"/>
				<File_NewLibrary/>
				<File_NewFolder i="NewFolder_16x"/>
				<File_Templates dd=""/>
				<File_Import t="Import..." i="open"/>
			</File_New>
			<sep/>
			<File_Disable/>
			<File_Rename hkText="F2"/>
			<File_Delete i="delete" hkText="Delete"/>
			<File_Properties t="Properties..." i="properties" hk="Ctrl+P"/>
			<File_OpenClose>
				<File_Open/>
				<File_OpenInNewWindow/>
				<File_OpenInDefaultApp/>
				<File_SelectInExplorer/>
				<sep/>
				<File_Close/>
				<File_CloseAll/>
				<File_CollapseFolders/>
				<sep/>
				<File_PreviousDocument i="back"/>
			</File_OpenClose>
			<File_More>
				<File_Cut hkText="Ctrl+X"/>
				<File_Copy hkText="Ctrl+C"/>
				<File_Paste hkText="Ctrl+V"/>
				<sep/>
				<File_CopyRelativePath/>
				<File_CopyFullPath/>
				<sep/>
				<File_PrintSetup t="Print Page Setup..."/>
				<File_Print t="Print..."/>
			</File_More>
			<sep/>
			<File_Collection>
				<File_RecentCollections dd=""/>
				<File_OpenCollection t="Open Collection..."/>
				<File_NewCollection t="New Collection..."/>
				<File_ExportCollection t="Export Selected as Collection..."/>
				<File_ImportCollection t="Import Collection..."/>
				<sep/>
				<File_FindInCollections t="Find in Other Collections..."/>
				<File_CollectionProperties t="Collection Properties..."/>
				<File_SaveAllNow i="save" hk="Ctrl+S"/>
			</File_Collection>
			<sep/>
			<File_Exit hkText="Alt+F4"/>
		</Menu_File>
		<Menu_Edit>
			<Edit_Undo i="undo"/>
			<Edit_Redo i="redo"/>
			<sep/>
			<Edit_Cut i="cut"/>
			<Edit_Copy i="copy"/>
			<Edit_Paste i="paste"/>
			<sep/>
			<Edit_Find i="find"/>
			<Edit_FindReferences/>
			<sep/>
			<Edit_GoToDefinition/>
			<Edit_PeekDefinition/>
			<Edit_Members/>
			<Edit_ContextHelp/>
			<sep/>
			<Edit_Lines>
				<Edit_Indent/>
				<Edit_Unindent/>
				<Edit_Comment/>
				<Edit_Uncomment/>
				<Edit_HideRegion/>
				<Edit_SelectAll/>
			</Edit_Lines>
			<Edit_View>
				<Edit_ImagesInCode i="images"/>
				<Edit_WrapLines i="annotations"/>
				<Edit_LineNumbers/>
				<Edit_IndentationGuides/>
			</Edit_View>
		</Menu_Edit>
		<Menu_Run>
			<Run_Run i="run"/>
			<Run_End i="deb_end"/>
			<Run_Pause i="back2"/>
			<Run_Compile i="compile"/>
			<sep/>
			<Run_AutoMinimize/>
			<Run_DisableTriggers/>
			<sep/>
			<Run_Debug>
				<Debug_RunToBreakpoint i="deb_run"/>
				<Debug_RunToCursor i="deb_cursor"/>
				<Debug_StepInto i="deb_into"/>
				<Debug_StepOver i="deb_next"/>
				<Debug_StepOut i="deb_out"/>
			</Run_Debug>
			<Debug_Breakpoints>
				<Debug_ToggleBreakpoint/>
				<Debug_PersistentBreakpoint/>
				<Debug_ClearLocalBreakpoints/>
				<Debug_ClearAllBreakpoints/>
			</Debug_Breakpoints>
			<Debug_DebugOptions/>
			<sep/>
			<Run_MakeExe t="Make Exe..."/>
		</Menu_Run>
		<Menu_Tools>
			<Tools_Record i="record"/>
			<Tools_RecordOther>
				<Tools_RecordMenu/>
				<Tools_RecordSingleAction/>
			</Tools_RecordOther>
			<sep/>
			<Tools_Panels dd=""/>
			<Tools_Toolbars dd=""/>
			<Tools_FilesAndTriggers i="mm"/>
			<Tools_DialogEditor i="dialog"/>
			<Tools_ToolbarEditor/>
			<Tools_MenuEditor/>
			<Tools_ImagelistEditor/>
			<Tools_HelpEditor i="tip"/>
			<Tools_Resources i="resources"/>
			<Tools_Icons i="icons"/>
			<Tools_RegularExpressions/>
			<Tools_ExploreWindows/>
			<Tools_RemapKeys/>
			<Tools_Components/>
			<Tools_Portable/>
			<sep/>
			<Tools_Output>
				<Tools_Output_Clear/>
				<Tools_Output_Copy/>
				<Tools_Output_FindSelectedText/>
				<Tools_Output_History/>
				<sep/>
				<Tools_Output_LogWindowEvents/>
				<Tools_Output_LogAccEvents/>
				<sep/>
				<Tools_Output_WrapLines/>
				<Tools_Output_WhiteSpace/>
				<sep/>
				<Tools_Output_Topmost/>
			</Tools_Output>
			<Tools_StatusBar>
				<Tools_Statusbar_Floating/>
				<Tools_Statusbar_MouseInfo/>
				<Tools_Statusbar_AutoHeight/>
				<Tools_Statusbar_SendToOutput/>
			</Tools_StatusBar>
			<sep/>
			<Tools_Options i="options"/>
		</Menu_Tools>
		<Menu_Help>
			<Help_QuickStart/>
			<Help_Reference i="help"/>
			<Help_ContextHelp/>
			<sep/>
			<Help_Web>
				<Help_Download/>
				<Help_Forum/>
				<Help_Email/>
			</Help_Web>
			<sep/>
			<Help_Donate/>
			<Help_About/>
		</Menu_Help>
	</Menu>
	<!--
	Standard toolbars.
	Users cannot add/delete buttons.
	Users can reorder buttons: Alt + drag-drop.
	Users can copy buttons to custom toolbars.
	Users can change some button properties.
	-->
	<File>
		<File_NewFolder i="NewFolder_16x"/>
		<File_NewScript i="_new" dd="File_New"/>
		<File_Properties i="properties"/>
		<File_PreviousDocument i="back"/>
		<File_SaveAllNow i="save"/>
	</File>
	<Edit>
		<Edit_Find i="find"/>
		<sep/>
		<Edit_Undo i="undo"/>
		<Edit_Redo i="redo"/>
		<sep/>
		<Edit_Cut i="cut"/>
		<Edit_Copy i="copy"/>
		<Edit_Paste i="paste"/>
		<sep/>
		<Edit_ImagesInCode i="images"/>
		<Edit_WrapLines i="annotations"/>
	</Edit>
	<Run>
		<Run_Compile i="compile"/>
		<Run_Run i="run"/>
		<Run_End i="deb_end"/>
		<Run_Pause i="back2"/>
		<sep/>
		<Debug_RunToBreakpoint i="deb_run"/>
		<Debug_RunToCursor i="deb_cursor"/>
		<Debug_StepInto i="deb_into"/>
		<Debug_StepOver i="deb_next"/>
		<Debug_StepOut i="deb_out"/>
	</Run>
	<Tools>
		<Tools_Record i="record"/>
		<Tools_FilesAndTriggers i="mm"/>
		<Tools_DialogEditor i="dialog"/>
		<Tools_Resources i="resources"/>
		<Tools_Icons i="icons"/>
		<Tools_Options i="options"/>
	</Tools>
	<Help>
		<Help_Reference i="help"/>
		<!-- note: don't use combo, it adds ~12 ms of startup time -->
		<Help_Find type="edit" cue="Find help, functions, tools" tt="Enter - search.
Esc - focus the code editor.
Down arrow - recent.
Middle click - clear.
F1 - function help.
F2 - function definition."/>
	</Help>

	<!--
	Custom toolbars.
	Users can add, delete and reorder buttons, change some button properties.
	Can be 0, 1 or 2 custom toolbars. Must be named Custom1 and Custom2.
	-->
	<Custom1/>
	<Custom2/>
</strips>