<panels>
	<split hor="" w1="20" splitter="1">
		<!--toolbars-->
		<split hor="" w1="10" splitter="1">
			<!--row1-->
			<split f="0.4">
				<panel name="Menu" />
				<split f="0.5">
					<panel name="Help" />
					<split f="0.99">
						<panel name="Tools" />
						<panel name="Custom1" />
					</split>
				</split>
			</split>
			<!--row2-->
			<split w1="200">
				<panel name="File" />
				<split w1="120">
					<panel name="Run" />
					<split w1="300">
						<panel name="Edit" />
						<panel name="Custom2" />
					</split>
				</split>
			</split>
		</split>
		<!--panels-->
		<split w1="200">
			<panel name="Files" captionAt="Top" />
			<split hor="" w2="150">
				<split w2="130">
					<panel name="Code" doc="" />
					<split hor="" f="0.5">
						<panel name="Open" captionAt="Top" />
						<tab captionAt="Top">
							<panel name="Running" />
							<panel name="Recent" />
						</tab>
					</split>
				</split>
				<tab>
					<panel name="Output" />
					<panel name="Find" />
					<panel name="Results" />
				</tab>
			</split>
		</split>
	</split>
</panels>